{
  "manifest_version": 3,
  "name": "Rolodex Capture",
  "version": "3.0.0", 
  "description": "Capture products for Rolodex web app",
  "permissions": ["contextMenus", "tabs"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png", 
    "128": "icon128.png"
  }
}